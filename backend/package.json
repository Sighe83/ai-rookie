{
  "name": "ai-rookie-backend",
  "version": "1.0.0",
  "description": "Backend API for AI Rookie platform - Supabase Optimized",
  "main": "src/server.js",
  "scripts": {
    "dev": "nodemon src/server.js",
    "start": "node src/server.js",
    "build": "echo 'No build step required'",
    "postinstall": "npx prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:generate": "prisma generate",
    "db:deploy": "prisma migrate deploy",
    "db:seed": "node src/database/seed.js",
    "db:reset": "prisma migrate reset --force",
    "db:setup:dev": "bash scripts/setup-dev-db.sh",
    "db:setup:prod": "bash scripts/setup-prod-db.sh",
    "db:deploy:all": "bash scripts/deploy-databases.sh",
    "db:test": "node scripts/test-db-connections.js",
    "test": "jest"
  },
  "dependencies": {
    "@prisma/client": "^5.7.0",
    "@supabase/supabase-js": "^2.39.0",
    "axios": "^1.11.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "joi": "^17.11.0",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.7",
    "pg": "^8.16.3",
    "stripe": "^18.3.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "prisma": "^5.7.0",
    "supertest": "^6.3.3"
  },
  "keywords": [
    "ai",
    "tutoring",
    "booking",
    "api",
    "express",
    "supabase",
    "vercel",
    "saas"
  ],
  "author": "AI Rookie Team",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0 <21.0.0",
    "npm": ">=9.0.0"
  }
}
